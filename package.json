{
  "name": "meetup-parcel",
  "version": "1.0.0",
  "description": "Test Parcel and Meetup API",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all --parallel start:ui start:server",
    "start:ui": "parcel public/index.html -p 9000",
    "start:server": "node ./server/index.js",
    "build": "parcel build ./public/index.html --out-dir ./server/public"
  },
  "keywords": [
    "parcel",
    "javascript",
    "react",
    "reactjs",
    "meetup",
    "js"
  ],
  "author": "@facosta0787",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.0",
    "express-asyncify": "^1.0.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.0",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.3"
  }
}
